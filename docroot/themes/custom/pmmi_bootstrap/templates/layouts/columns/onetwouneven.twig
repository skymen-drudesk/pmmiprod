{#
/**
* @file
* Template for a 2-up (1 row of 2 columns) layout.
*/
#}
<div {{ content['#attrs'].addClass('layout-2upuneven clearfix') }}>
<div class="region-top">
    {{ content.top }}
</div>
<div class="rows container">
    <div class="row-1 row">
        {{ field_render([content.oneone, 'col col-1 col-sm-12 col-md-5 col-lg-5 col-xl-5']) }}
        {{ field_render([content.onetwo, 'col col-2 col-sm-12 col-md-7 col-lg-7 col-xl-7']) }}
    </div>
</div>
<div class="region-bottom">
    {{ content.bottom }}
</div>
</div>
