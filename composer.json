    }
  },
  "scripts": {
    "post-install-cmd": [
      "@composer drupal-scaffold"
    ],
    "post-update-cmd": [
      "@composer drupal-scaffold"
    ],
    "drupal-scaffold": [
      "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "installer-paths": {
      "docroot/core": [
        "type:drupal-core"
      ],
      "docroot/libraries/{$name}": [
        "type:drupal-library"
      ],
      "docroot/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "docroot/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "docroot/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "docroot/drush/commands/{$name}": [
        "type:drupal-drush"
      ]
    },
    "preserve-paths": [
      "docroot/sites/all/modules/contrib",
      "docroot/sites/all/modules/custom",
      "docroot/sites/all/modules/feature_modules",
      "docroot/sites/all/themes/contrib",
      "docroot/sites/all/libraries",
      "docroot/sites/default/settings.php",
      "docroot/sites/default/files"
    ],
    "drupal-scaffold": {
      "excludes": [
        ".htaccess"
      ]
    },
    "merge-plugin": {
      "include": [
        "docroot/modules/custom/pmmi_crawler_detect/composer.json",
        "docroot/modules/custom/pmmi_twitter/composer.json",
        "docroot/modules/custom/pmmi_facebook/composer.json",
        "docroot/modules/custom/pmmi_sso/composer.json"
      ],
      "recurse": false,
      "replace": false,
      "merge-extra": false
    },
    "enable-patching": true,
    "patches-file": "composer.patches.json"
  }
}
